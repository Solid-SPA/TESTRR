      <template id="footer_custom_website" inherit_id="website.footer_custom">
      
      <xpath expr="//footer//span[hasclass('o_footer_copyright_name')]" position="replace">
          <span class="mr-2 o_footer_copyright_name">
            <font style="font-size: 8px;" class="text-600">Servicios Automoviles Greep 2023 Â©&amp;nbsp; - All rights reserved. -&amp;nbsp;<a href="/politicas" target="_blank" data-original-title="" title="" aria-describedby="tooltip638273">Politicas de privacidade</a>&amp;nbsp;- <a href="https://greep.cl/" target="blank"> Desarrollo Odoo / Greep</a></font>
          </span>
        </xpath>

         <xpath expr="//footer[@id='bottom']" position="attributes">
          <attribute name="t-attf-class" add="#{'d-none o_snippet_invisible' if 'footer_visible' in main_object and not main_object.footer_visible else ''}" separator=" "/>
        </xpath>
    </template>


  </data>
</odoo>